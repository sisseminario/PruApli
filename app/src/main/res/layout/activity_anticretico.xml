<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Anticretico">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.design.widget.BottomNavigationView
            android:id="@+id/menunave"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="8dp"

            android:layout_marginTop="8dp"
            android:background="?android:windowBackground"
            android:backgroundTint="@color/sobrecabeza"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:menu="@menu/navegation_venta_menu"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"></android.support.design.widget.BottomNavigationView>
    </android.support.constraint.ConstraintLayout>


</RelativeLayout>